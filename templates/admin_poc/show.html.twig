{% extends 'base.html.twig' %}

{% block title %}Poc{% endblock %}

{% block body %}
    <h1>Poc</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ poc.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ poc.name }}</td>
            </tr>
            <tr>
                <th>ImageFilename</th>
                <td>{{ poc.imageFilename }}</td>
            </tr>
            <tr>
                <th>GithubLink</th>
                <td>{{ poc.githubLink }}</td>
            </tr>
            <tr>
                <th>LiveDemoLink</th>
                <td>{{ poc.liveDemoLink }}</td>
            </tr>
            <tr>
                <th>Keywords</th>
                <td>{{ poc.keywords }}</td>
            </tr>
            <tr>
                <th>Premium (1=yes)</th>
                <td>{{ poc.isPremium }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_poc_index') }}">back to list</a>

    <a href="{{ path('admin_poc_edit', {'id': poc.id}) }}">edit</a>

    {{ include('admin_poc/_delete_form.html.twig') }}
{% endblock %}
